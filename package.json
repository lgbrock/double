  "license": "ISC",
  "scripts": {
    "once": "npm run git-init && npm i && npm run db-migrate && npm run db-seed",
    "git-init": "git init && git add . && git commit -m 'ðŸŽ‰ Challenge accepted!'",
    "db-migrate": "knex migrate:latest --knexfile ./src/db/config.ts",
