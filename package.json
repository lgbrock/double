    "lint": "prettier --write src",
    "start": "ts-node-dev --respawn --transpile-only src/index.ts"
  },
  "dependencies": {
    "apollo-server": "^3.6.2",
    "axios": "^0.25.0",
    "graphql": "^16.3.0",
    "knex": "^0.95.15",
    "sqlite3": "^5.0.2",
    "vscode-sqlite3": "^4.0.11"
  },
  "devDependencies": {
    "prettier": "^2.5.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.5"
  },
  "main": "index.ts"
}
